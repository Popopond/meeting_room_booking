<div id="<%= dom_id booking %>">
  <p>
    <strong>User:</strong>
    <%= booking.user_id %>
  </p>

  <p>
    <strong>Room:</strong>
    <%= booking.room_id %>
  </p>

  <p>
    <strong>Start time:</strong>
    <%= booking.start_time %>
  </p>

  <p>
    <strong>End time:</strong>
    <%= booking.end_time %>
  </p>

  <p>
    <strong>Created at:</strong>
    <%= booking.created_at %>
  </p>

  <p>
    <strong>Complete:</strong>
    <%= booking.complete %>
  </p>

  <p>
    <strong>Confirmation code:</strong>
    <%= booking.confirmation_code %>
  </p>

</div>
